<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="membership" translate="label" type="text" sortOrder="1" showInDefault="1">
            <class>separator-top</class>
            <label>Membership</label>
            <tab>customer</tab>
            <resource>Magento_LoginAsCustomer::config_section</resource>
            <group id="membership_group_id" translate="label" type="text" sortOrder="1" showInDefault="1" canRestore="1">
                <label>Membership</label>
                <field id="enabled" translate="label" type="select" sortOrder="10" showInDefault="1" canRestore="1">
                    <label>Enable Extension</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
    </system>
</config>
